{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            [
                "app",
                "layout",
                "com",
                "page",
                "pl",
                "ant"
            ],
            "kebab-case"
        ],
    }
}